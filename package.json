{
  "name": "magicalc",
  "description": "magic calculator",
  "repository": {
    "type": "git",
    "url": "git://github.com/christianhg/magicalc.git"
  },
  "version": "0.0.1",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" \"npm run scss\" \"npm run watch:scss\"",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "scss": "node-sass --output-style compressed scss/styles.scss styles.css",
    "typings": "typings",
    "postinstall": "typings install",
    "watch:scss": "onchange 'scss/**/*.scss' -- npm run scss"
  },
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.1.0",
    "node-sass": "^3.4.2",
    "onchange": "^2.2.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.12"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.13",
    "es6-shim": "^0.35.0",
    "lodash": "^3.10.1",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.4",
    "systemjs": "0.19.25",
    "zone.js": "0.6.8"
  }
}
